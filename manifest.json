{
	"manifest_version": 2,
	"name": "Github Merge Helper",
	"version": "0.0.2",
	"icons": {
		"128": "icon-128.png" 
	},
	"browser_action": {
		"default_icon": "icon-32.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://github.com/*/*/pull/*"
			],
			"js": ["gmh.js"],
			"run_at": "document_start"
		}
	]
}